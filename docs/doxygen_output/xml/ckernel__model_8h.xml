        <type>int</type>
        <definition>int ck_model_load_weights_flat</definition>
        <argsstring>(TransformerModel *m, const char *path)</argsstring>
        <name>ck_model_load_weights_flat</name>
        <param>
          <type><ref refid="structTransformerModel" kindref="compound">TransformerModel</ref> *</type>
          <declname>m</declname>
        </param>
        <param>
          <type>const char *</type>
